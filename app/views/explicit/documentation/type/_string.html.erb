<%= type_constraints do %>
  <% if type.empty == false %>
    <%= type_constraint "not", "empty" %>
  <% end %>

  <% if type.min_length %>
    <%= type_constraint "min_length", type.min_length %>
  <% end %>

  <% if type.max_length %>
    <%= type_constraint "max_length", type.max_length %>
  <% end %>

  <% if type.format %>
    <%= type_constraint "format", type.format.inspect %>
  <% end %>

  <% if type.downcase %>
    <%= type_constraint "case", "insensitive" %>
  <% end %>
<% end %>
